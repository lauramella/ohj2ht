<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Label alignment="CENTER" text="Add new set" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="20.0" top="20.0" />
         </BorderPane.margin>
      </Label>
   </top>
   <bottom>
      <HBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="Save">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button layoutX="84.0" layoutY="48.0" mnemonicParsing="false" text="Back">
               <HBox.margin>
                  <Insets bottom="20.0" left="10.0" right="20.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
   <center>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Label text="Name">
               <HBox.margin>
                  <Insets bottom="10.0" left="20.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Label>
            <TextField>
               <HBox.margin>
                  <Insets bottom="5.0" right="20.0" top="5.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
   </center>
</BorderPane>
